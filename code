@bot.command(pass_context=True)
async def eblan(ctx, arg:int):
  i = arg
  while i != 0 :
    await ctx.send('https://memepedia.ru/wp-content/uploads/2018/07/ya-eblan-rastyanutyy.jpg')
    i -= 1

